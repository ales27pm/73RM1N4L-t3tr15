{
  "cli": {
    "version": "^16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "credentialsSource": "local",
      "hooks": {
        "pre-build": "npm run android:keystore"
      },
      "env": {},
      "ios": {
        "distribution": "store",
        "autoIncrement": true,
        "prebuildCommand": "bash scripts/eas-prebuild-ios.sh"
      },
      "android": {
        "distribution": "store"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {}
    }
  }
}
