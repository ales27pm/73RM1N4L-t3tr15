{
  "cli": {
    "version": "^16.17.4"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "credentialsSource": "remote",
      "env": {},
      "ios": {
        "distribution": "store",
        "autoIncrement": true,
        "prebuildCommand": "bash scripts/eas-prebuild-ios.sh"
      },
      "android": {
        "distribution": "store"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {}
    }
  }
}
