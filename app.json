{
  "expo": {
    "name": "Terminal t3tr15",
    "slug": "terminal-t3tr15",
    "scheme": "netsight",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "icon": "./icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#050b1f",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vibecode.terminaltetris-u5zt2g",
      "splash": {
        "image": "./assets/splash.png",
        "backgroundColor": "#050b1f",
        "resizeMode": "contain"
      },
      "buildNumber": "4",
      "infoPlist": {
        "NSSpeechRecognitionUsageDescription": "Terminal t3tr15 uses speech recognition to turn your spoken words into text inside the app.",
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.netsight",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#050b1f"
      }
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "0661cabd-28d1-403d-ad26-212dddd78e58"
      }
    }
  }
}
